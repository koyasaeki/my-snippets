  "gcd_function": {
    "prefix": "pg:gcd_function",
    "body": [
      "fn gcd(m: usize, n: usize) -> usize {",
      "    match n {",
      "        0 => return m,",
      "        _ => return gcd(n, m % n),",
      "    }",
      "}"
    ]
  }
